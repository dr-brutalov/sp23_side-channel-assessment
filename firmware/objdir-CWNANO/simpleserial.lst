 336:../firmware/simpleserial/simpleserial.c **** 	char c;
 337:../firmware/simpleserial/simpleserial.c **** 
 338:../firmware/simpleserial/simpleserial.c **** 	// Find which command we're receiving
 339:../firmware/simpleserial/simpleserial.c **** 	c = getch();
 340:../firmware/simpleserial/simpleserial.c **** 
 341:../firmware/simpleserial/simpleserial.c **** 	int cmd;
 342:../firmware/simpleserial/simpleserial.c **** 	for(cmd = 0; cmd < num_commands; cmd++)
 343:../firmware/simpleserial/simpleserial.c **** 	{
 344:../firmware/simpleserial/simpleserial.c **** 		if(commands[cmd].c == c)
 345:../firmware/simpleserial/simpleserial.c **** 			break;
 346:../firmware/simpleserial/simpleserial.c **** 	}
 347:../firmware/simpleserial/simpleserial.c **** 
 348:../firmware/simpleserial/simpleserial.c **** 	// If we didn't find a match, give up right away
 349:../firmware/simpleserial/simpleserial.c **** 	if(cmd == num_commands)
 350:../firmware/simpleserial/simpleserial.c **** 		return;
 351:../firmware/simpleserial/simpleserial.c **** 
 352:../firmware/simpleserial/simpleserial.c **** 	// If flag CMD_FLAG_LEN is set, the next byte indicates the sent length
 353:../firmware/simpleserial/simpleserial.c **** 	if ((commands[cmd].flags & CMD_FLAG_LEN) != 0)
