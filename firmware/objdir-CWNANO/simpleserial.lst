 202:../firmware/simpleserial/simpleserial.c **** void simpleserial_put(char c, uint8_t size, uint8_t* output)
 203:../firmware/simpleserial/simpleserial.c **** {
 204:../firmware/simpleserial/simpleserial.c **** 	uint8_t data_buf[MAX_SS_LEN];
 205:../firmware/simpleserial/simpleserial.c **** 	data_buf[0] = 0x00;
 206:../firmware/simpleserial/simpleserial.c **** 	data_buf[1] = c;
 207:../firmware/simpleserial/simpleserial.c **** 	data_buf[2] = size;
 208:../firmware/simpleserial/simpleserial.c **** 	int i = 0;
 209:../firmware/simpleserial/simpleserial.c **** 	for (; i < size; i++) {
 210:../firmware/simpleserial/simpleserial.c **** 		data_buf[i + 3] = output[i];
 211:../firmware/simpleserial/simpleserial.c **** 	}
 212:../firmware/simpleserial/simpleserial.c **** 	data_buf[i + 3] = ss_crc(data_buf+1, size+2);
 213:../firmware/simpleserial/simpleserial.c **** 	data_buf[i + 4] = 0x00;
 214:../firmware/simpleserial/simpleserial.c **** 	stuff_data(data_buf, i + 5);
 215:../firmware/simpleserial/simpleserial.c **** 	for (int i = 0; i < size + 5; i++) {
 216:../firmware/simpleserial/simpleserial.c **** 		putch(data_buf[i]);
 217:../firmware/simpleserial/simpleserial.c **** 	}
 218:../firmware/simpleserial/simpleserial.c **** }
