 355:../firmware/simpleserial/simpleserial.c **** 		uint8_t l = 0;
 356:../firmware/simpleserial/simpleserial.c **** 		char buff[2];
 357:../firmware/simpleserial/simpleserial.c **** 		buff[0] = getch();
 358:../firmware/simpleserial/simpleserial.c **** 		buff[1] = getch();
 359:../firmware/simpleserial/simpleserial.c **** 		if (hex_decode(1, buff, &l))
 360:../firmware/simpleserial/simpleserial.c **** 			return;
 361:../firmware/simpleserial/simpleserial.c **** 		commands[cmd].len = l;
 362:../firmware/simpleserial/simpleserial.c **** 	}
 363:../firmware/simpleserial/simpleserial.c **** 
 364:../firmware/simpleserial/simpleserial.c **** 	// Receive characters until we fill the ASCII buffer
 365:../firmware/simpleserial/simpleserial.c **** 	for(int i = 0; i < 2*commands[cmd].len; i++)
 366:../firmware/simpleserial/simpleserial.c **** 	{
 367:../firmware/simpleserial/simpleserial.c **** 		c = getch();
 368:../firmware/simpleserial/simpleserial.c **** 
 369:../firmware/simpleserial/simpleserial.c **** 		// Check for early \n
 370:../firmware/simpleserial/simpleserial.c **** 		if(c == '\n' || c == '\r')
 371:../firmware/simpleserial/simpleserial.c **** 			return;
 372:../firmware/simpleserial/simpleserial.c **** 
 373:../firmware/simpleserial/simpleserial.c **** 		ascii_buf[i] = c;
 374:../firmware/simpleserial/simpleserial.c **** 	}
 375:../firmware/simpleserial/simpleserial.c **** 
 376:../firmware/simpleserial/simpleserial.c **** 	// Assert that last character is \n or \r
 377:../firmware/simpleserial/simpleserial.c **** 	c = getch();
 378:../firmware/simpleserial/simpleserial.c **** 	if(c != '\n' && c != '\r')
 379:../firmware/simpleserial/simpleserial.c **** 		return;
 380:../firmware/simpleserial/simpleserial.c **** 
 381:../firmware/simpleserial/simpleserial.c **** 	// ASCII buffer is full: convert to bytes
 382:../firmware/simpleserial/simpleserial.c **** 	// Check for illegal characters here
 383:../firmware/simpleserial/simpleserial.c **** 	if(hex_decode(commands[cmd].len, ascii_buf, data_buf))
 384:../firmware/simpleserial/simpleserial.c **** 		return;
 385:../firmware/simpleserial/simpleserial.c **** 
 386:../firmware/simpleserial/simpleserial.c **** 	// Callback
 387:../firmware/simpleserial/simpleserial.c **** 	uint8_t ret[1];
 388:../firmware/simpleserial/simpleserial.c **** 	ret[0] = commands[cmd].fp(data_buf, commands[cmd].len);
 389:../firmware/simpleserial/simpleserial.c **** 
 390:../firmware/simpleserial/simpleserial.c **** 	// Acknowledge (if version is 1.1)
 391:../firmware/simpleserial/simpleserial.c **** #if SS_VER == SS_VER_1_1
 392:../firmware/simpleserial/simpleserial.c **** 	simpleserial_put('z', 1, ret);
 393:../firmware/simpleserial/simpleserial.c **** #endif
 394:../firmware/simpleserial/simpleserial.c **** }
