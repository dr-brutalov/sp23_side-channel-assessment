  78:../firmware/simpleserial/simpleserial.c **** uint8_t stuff_data(uint8_t *buf, uint8_t len)
  79:../firmware/simpleserial/simpleserial.c **** {
  80:../firmware/simpleserial/simpleserial.c **** 	uint8_t i = 1;
  81:../firmware/simpleserial/simpleserial.c **** 	uint8_t last = 0;
  82:../firmware/simpleserial/simpleserial.c **** 	for (; i < len; i++) {
  83:../firmware/simpleserial/simpleserial.c **** 		if (buf[i] == FRAME_BYTE) {
  84:../firmware/simpleserial/simpleserial.c **** 			buf[last] = i - last;
  85:../firmware/simpleserial/simpleserial.c **** 			last = i;
  86:../firmware/simpleserial/simpleserial.c **** 		}
  87:../firmware/simpleserial/simpleserial.c **** 	}
  88:../firmware/simpleserial/simpleserial.c **** 	return 0x00;
  89:../firmware/simpleserial/simpleserial.c **** }
  90:../firmware/simpleserial/simpleserial.c **** 
  91:../firmware/simpleserial/simpleserial.c **** uint8_t unstuff_data(uint8_t *buf, uint8_t len)
  92:../firmware/simpleserial/simpleserial.c **** {
  93:../firmware/simpleserial/simpleserial.c **** 	uint8_t next = buf[0];
  94:../firmware/simpleserial/simpleserial.c **** 	buf[0] = 0x00;
  95:../firmware/simpleserial/simpleserial.c **** 	//len -= 1;
  96:../firmware/simpleserial/simpleserial.c **** 	uint8_t tmp = next;
  97:../firmware/simpleserial/simpleserial.c **** 	while ((next < len) && tmp != 0) {
  98:../firmware/simpleserial/simpleserial.c **** 		tmp = buf[next];
  99:../firmware/simpleserial/simpleserial.c **** 		buf[next] = FRAME_BYTE;
 100:../firmware/simpleserial/simpleserial.c **** 		next += tmp;
 101:../firmware/simpleserial/simpleserial.c **** 	}
 102:../firmware/simpleserial/simpleserial.c **** 	return next;
 103:../firmware/simpleserial/simpleserial.c **** }
