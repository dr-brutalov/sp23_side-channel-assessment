 133:../firmware/simpleserial/simpleserial.c **** void simpleserial_get(void)
 134:../firmware/simpleserial/simpleserial.c **** {
 135:../firmware/simpleserial/simpleserial.c **** 	uint8_t data_buf[MAX_SS_LEN];
 136:../firmware/simpleserial/simpleserial.c **** 	uint8_t err = 0;
 137:../firmware/simpleserial/simpleserial.c **** 
 138:../firmware/simpleserial/simpleserial.c **** 	for (int i = 0; i < 4; i++) {
 139:../firmware/simpleserial/simpleserial.c **** 		data_buf[i] = getch(); //PTR, cmd, scmd, len
 140:../firmware/simpleserial/simpleserial.c **** 		if (data_buf[i] == FRAME_BYTE) {
 141:../firmware/simpleserial/simpleserial.c **** 			err = SS_ERR_FRAME_BYTE;
 142:../firmware/simpleserial/simpleserial.c **** 			goto ERROR;
 143:../firmware/simpleserial/simpleserial.c **** 		}
 144:../firmware/simpleserial/simpleserial.c **** 	}
 145:../firmware/simpleserial/simpleserial.c **** 	uint8_t next_frame = unstuff_data(data_buf, 4);
 146:../firmware/simpleserial/simpleserial.c **** 
 147:../firmware/simpleserial/simpleserial.c **** 	// check for valid command
 148:../firmware/simpleserial/simpleserial.c **** 	uint8_t c = 0;
 149:../firmware/simpleserial/simpleserial.c **** 	for(c = 0; c < num_commands; c++)
 150:../firmware/simpleserial/simpleserial.c **** 	{
 151:../firmware/simpleserial/simpleserial.c **** 		if(commands[c].c == data_buf[1])
 152:../firmware/simpleserial/simpleserial.c **** 			break;
 153:../firmware/simpleserial/simpleserial.c **** 	}
 154:../firmware/simpleserial/simpleserial.c **** 
 155:../firmware/simpleserial/simpleserial.c **** 	if (c == num_commands) {
 156:../firmware/simpleserial/simpleserial.c **** 		err = SS_ERR_CMD;
 157:../firmware/simpleserial/simpleserial.c **** 		goto ERROR;
 158:../firmware/simpleserial/simpleserial.c **** 	}
 159:../firmware/simpleserial/simpleserial.c **** 
 160:../firmware/simpleserial/simpleserial.c **** 	//check that next frame not beyond end of message
 161:../firmware/simpleserial/simpleserial.c **** 	// account for cmd, scmd, len, data, crc, end of frame
 162:../firmware/simpleserial/simpleserial.c **** 	if ((data_buf[3] + 5) < next_frame) {
 163:../firmware/simpleserial/simpleserial.c **** 		err = SS_ERR_LEN;
 164:../firmware/simpleserial/simpleserial.c **** 		goto ERROR;
 165:../firmware/simpleserial/simpleserial.c **** 	}
 166:../firmware/simpleserial/simpleserial.c **** 
 167:../firmware/simpleserial/simpleserial.c **** 	// read in data
 168:../firmware/simpleserial/simpleserial.c **** 	// eq to len + crc + frame end
 169:../firmware/simpleserial/simpleserial.c **** 	int i = 4;
 170:../firmware/simpleserial/simpleserial.c **** 	for (; i < data_buf[3] + 5; i++) {
 171:../firmware/simpleserial/simpleserial.c **** 		data_buf[i] = getch();
 172:../firmware/simpleserial/simpleserial.c **** 		if (data_buf[i] == FRAME_BYTE) {
 173:../firmware/simpleserial/simpleserial.c **** 			err = SS_ERR_FRAME_BYTE;
 174:../firmware/simpleserial/simpleserial.c **** 			goto ERROR;
 175:../firmware/simpleserial/simpleserial.c **** 		}
 176:../firmware/simpleserial/simpleserial.c **** 	}
 177:../firmware/simpleserial/simpleserial.c **** 
 178:../firmware/simpleserial/simpleserial.c **** 	//check that final byte is the FRAME_BYTE
 179:../firmware/simpleserial/simpleserial.c **** 	data_buf[i] = getch();
 180:../firmware/simpleserial/simpleserial.c **** 	if (data_buf[i] != FRAME_BYTE) {
 181:../firmware/simpleserial/simpleserial.c **** 		err = SS_ERR_LEN;
 182:../firmware/simpleserial/simpleserial.c **** 		goto ERROR;
 183:../firmware/simpleserial/simpleserial.c **** 	}
 184:../firmware/simpleserial/simpleserial.c **** 
 185:../firmware/simpleserial/simpleserial.c **** 	//fully unstuff data now
 186:../firmware/simpleserial/simpleserial.c **** 	unstuff_data(data_buf + next_frame, i - next_frame + 1);
 187:../firmware/simpleserial/simpleserial.c **** 
 188:../firmware/simpleserial/simpleserial.c **** 	//calc crc excluding original frame offset and frame end and crc
 189:../firmware/simpleserial/simpleserial.c **** 	uint8_t crc = ss_crc(data_buf+1, i-2);
 190:../firmware/simpleserial/simpleserial.c **** 	if (crc != data_buf[i-1]) {
 191:../firmware/simpleserial/simpleserial.c **** 		err = SS_ERR_CRC;
 192:../firmware/simpleserial/simpleserial.c **** 		goto ERROR;
 193:../firmware/simpleserial/simpleserial.c **** 	}
 194:../firmware/simpleserial/simpleserial.c **** 
 195:../firmware/simpleserial/simpleserial.c **** 	err = commands[c].fp(data_buf[1], data_buf[2], data_buf[3], data_buf+4);
 196:../firmware/simpleserial/simpleserial.c **** 
 197:../firmware/simpleserial/simpleserial.c **** ERROR:
 198:../firmware/simpleserial/simpleserial.c **** 	simpleserial_put('e', 0x01, &err);
 199:../firmware/simpleserial/simpleserial.c **** 	return;
 200:../firmware/simpleserial/simpleserial.c **** }
